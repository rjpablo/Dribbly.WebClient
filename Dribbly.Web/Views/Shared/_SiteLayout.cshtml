    <script type="text/javascript">
        window.Dribbly = {};
        window.Dribbly.clientSettings = @Html.Raw(Json.Serialize(Model.clientSettings));
        window.Dribbly.logClientException = function (message, errorMsg, url, errorCode, stackTrace, lineNo, column) {

            try {
                if (!window.Dribbly.userPlatform) {
                    var result = bowser.getParser(window.navigator.userAgent);
                    window.Dribbly.userPlatform = {
                        browser: result.parsedResult.browser.name + ' ' + result.parsedResult.browser.version,
                        os: result.parsedResult.os.name + ' ' + result.parsedResult.os.versionName
                    };
                }
            }
            catch(err){
                window.Dribbly.userPlatform = {};
            }
            finally {
                return $.post(window.Dribbly.clientSettings.loggingServiceAddress, {
                    message: message,
                    errorMessage: errorMsg,
                    url: url,
                    errorCode: errorCode,
                    lineNo: lineNo,
                    column: column,
                    stack: stackTrace,
                    loggedBy: window.Dribbly.authentication ? window.Dribbly.authentication.userId : null,
                    browser: window.Dribbly.userPlatform.browser,
                    os: window.Dribbly.userPlatform.os
                });
            }

        }
    </script>

